@model IEnumerable<multishopbd.Models.Category>

@{ ViewBag.Title = "Search"; }
<h2>Results</h2>
<table class="table table-condensed table-striped">
    <tr>
        <th>Category ID</th>
        <th>Title</th>
    </tr>
    @foreach (var item in Model)
    {

        <tr>
            <td>@item.CategoryId</td>
            <td>@item.CategoryName</td>
    </tr>
    }
</table>
